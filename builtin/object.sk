class Object
  def ==(other: Object) -> Bool
    panic "[`==` not yet implemented for this class]"
    false
  end

  def inspect -> String
    self.to_s
  end

  def p(obj: Object)
    puts obj.inspect
  end

  def panic(msg: String)
    puts msg
    exit 1
  end

  def to_s -> String
    "#<?>"  # TODO:  "#<" + self.class.name + ">"
  end
end
